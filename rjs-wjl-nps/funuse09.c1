//test error_typecheck
// Good error message would look like:
// type fn not small
// [Hint: cannot pass or store functions in variables directly, use
// funciton pointers]

typedef bool fn(int x, int y);

bool eq(int x, int y) { return x == y; }

int main() {
  fn f = eq;
  int i = 0;
  i += f(0,0) ? 1 : 0;
  i += f(0,2) ? 2 : 0;
  return i;
}