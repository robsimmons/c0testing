//test error_typecheck
int add(int a, int b) {
  int c = a + 1;
  int d = b - 1;
  return c + d;
}

int mul(int a, int b) {
  if (a == 0) { return 0; }
  else { return add(b, mul(a - 1, b)); }
}

int main() {
  bool b;

  b = mul(0,5); // Type error: bool/int
  if (b != 0) return -1; 

  b = mul(6,0);
  if (b != 0) return -1;
  b = mul(1,5);
  if (b != 5) return -1;

  b = mul(6,1);
  if (b != 6) return -1; 

  b = mul(5,5);
  if (b != 25) return -1;

  return 1;
}
