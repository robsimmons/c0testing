//test error_typecheck

/* testing undefined function pointers interacting with conditional
 * expressions */

#use <string>

typedef
string s2s(string s);

string to_lower(string s);

string id(string s) {
  return s;
}

int main() {
  bool b = false;
  s2s* f = &to_lower;
  s2s* h = &id;
  s2s* g = b ? f : !b ? h : &id;
  return (f == g) ? 1 : 0;
}
