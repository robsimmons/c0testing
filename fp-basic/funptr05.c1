/* type checking of function pointers across conditionals */

typedef
int ii2i(int x, int y);

int add(int x, int y) { return x+y; }
int mult(int x, int y) { return x*y; }

int main() {
  ii2i* g;
  ii2i* f = &mult;
  int result = 0;
  for (int i = 0; i < 20; i++) {
    g = (i%2 == 0) ? &add : &mult;
    (i % 2 == 0) ? &add : &mult;
    f = (i%2 == 0) ? ((i%3 == 0) ? f : &add) : &mult;
    result = (*f)(result, i);
  }
  return result;
}
